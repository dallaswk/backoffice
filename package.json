{
  "name": "crud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:*",
    "start:css": "tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/tailwind.css --watch",
    "start:img": "onchange \"./src/assets/img-hi\" -- npm run build:img",
    "start:serve": "vue-cli-service serve",
    "build": "run-p build:*",
    "build:cli": "vue-cli-service build",
    "build:css": "tailwindcss -o src/assets/css/tailwind.css -m",
    "build:img": "imagemin ./src/assets/img-hi/* --plugin.mozjpeg.quality=50 --plugin.pngquant.quality={0.3,0.5} --out-dir=./src/assets/img",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "core-js": "^3.8.3",
    "element-ui": "^2.15.12",
    "firebase": "^9.14.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "babel-plugin-component": "^1.1.1",
    "cypress": "^9.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-template-compiler": "^2.6.14"
  }
}
